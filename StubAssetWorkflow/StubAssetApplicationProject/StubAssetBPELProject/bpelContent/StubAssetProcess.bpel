						</bpel:from>
						<bpel:to variable="mpxTransformPLRequest" part="parameters" />
					</bpel:copy>

				</bpel:assign>
			</bpel:sequence>
			<bpel:else>
				<bpel:sequence>
					<bpel:assign validate="no" name="AssignToGetTitles">
						<bpel:copy>
							<bpel:from>
								<bpel:literal>
									<ns0:GetVodTitle xmlns:ns0="http://service.skytv.co.nz/ibms/v1"
